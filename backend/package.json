{
  "name": "kelseys-cats-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "stop": "npx kill-port 5001 || echo 'No process running'",
    "update": "npm run stop && git pull origin main && npm run start",
    "migrate": "node scripts/run-migrations-safe.js",
    "migrate:reset": "node scripts/reset-migration-tracking.js",
    "migrate:fresh": "echo '⚠️  This will DROP ALL TABLES and recreate them!' && read -p 'Type YES to continue: ' confirm && [ \"$confirm\" = \"YES\" ] && mysql -u root -p kelseys_cats < scripts/fresh-start.sql && node scripts/create-admin.js",
    "create-admin": "node scripts/create-admin.js",
    "list-users": "node scripts/list-users.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.9.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "kill-port": "^2.0.1"
  }
}
