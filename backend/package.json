{
  "name": "kelseys-cats-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "hash-password": "node hash.js",
    "curl": "sh curl.sh",
    "migrate": "node scripts/run-migrations.js",
    "migrate:bash": "bash scripts/run-migrations.sh",
    "migrate:list": "ls -la migrations/",
    "update": "npm run stop && git pull origin main && npm run start",
    "update:dev": "npm run stop && git pull origin main && npm run dev",
    "stop": "npx kill-port 5001 || echo 'No process running'"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.9.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "kill-port": "^2.0.1"
  }
}
