{
  "name": "kelseys-cats-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "stop": "npx kill-port 5001 || echo 'No process running'",
    "update": "npm run stop && git pull origin main && npm run start",
    "migrate": "node scripts/run-migrations.js",
    "migrate:all": "node scripts/run-migrations.js",
    "migrate:bash": "bash scripts/run-migrations.sh",
    "migrate:list": "ls -la migrations/",
    "migrate:vfv-table": "node scripts/migrations/run-single-migration.js create_vfv_cats_table.sql",
    "migrate:view": "node scripts/migrations/run-single-migration.js add_featured_to_view.sql",
    "migrate:vfv-integration": "node scripts/migrations/run-single-migration.js add_vfv_to_view.sql",
    "migrate:adoptapet-url": "node scripts/migrations/run-single-migration.js add_adoptapet_url.sql",
    "migrate:adoptapet-id": "node scripts/migrations/run-single-migration.js add_adoptapet_id_to_vfv_cats.sql",
    "migrate:additional-images": "node scripts/migrations/run-single-migration.js add_additional_images_column.sql"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.9.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "kill-port": "^2.0.1"
  }
}
